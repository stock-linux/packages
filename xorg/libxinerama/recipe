name=libxinerama
version=1.1.5
release=1
description="X11 Xinerama extension library"
source=(https://www.x.org/pub/individual/lib/libXinerama-$version.tar.xz)
packager=Skythrew
makedepends=(libxext util-macros xorgproto)
unpack() {
    tar -xf libXinerama-$version.tar.xz
    cd libXinerama-$version
}
