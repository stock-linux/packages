name=libxdmcp
version=1.1.4
release=1
description="The libXdmcp package contains a library implementing the X Display Manager Control Protocol."
source=(https://www.x.org/pub/individual/lib/libXdmcp-$version.tar.xz)
packager=Skythrew
makedepends=(util-macros xorgproto)
unpack() {
    tar -xf libXdmcp-$version.tar.xz
    cd libXdmcp-$version
}
