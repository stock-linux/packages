name=libxcursor
version=1.2.1
release=1
description="X cursor management library"
source=(https://www.x.org/archive//individual/lib/libXcursor-$version.tar.gz)
packager=camini
makedepends=(xorgproto libxrender libxfixes libx11)
unpack(){
	tar -xf libXcursor-$version.tar.gz
	cd libXcursor-$version
}
