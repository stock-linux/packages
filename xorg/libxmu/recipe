name=libxmu
version=1.1.4
release=1
description="X11 miscellaneous micro-utility library"
source=(https://www.x.org/archive//individual/lib/libXmu-$version.tar.gz)
packager=camini
makedepends=(utils-macros xorgproto libxt libxext libx11)
unpack(){
	tar -xf libXmu-$version.tar.gz
	cd libXmu-$version
}
