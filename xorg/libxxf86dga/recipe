name=libxxf86dga
version=1.1.6
release=1
description="X.Org Xxf86dga library"
source=(https://www.x.org/pub/individual/lib/libXxf86dga-$version.tar.xz)
packager=Skythrew
makedepends=(util-macros xorgproto libxext libx11)
unpack() {
    tar -xf libXxf86dga-$version.tar.xz
    cd libXxf86dga-$version
}
